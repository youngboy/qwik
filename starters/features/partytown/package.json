{
  "description": "Use Partytown into your Qwik app",
  "devDependencies": {
    "@builder.io/partytown": "^0.7.0"
  },
  "__qwik__": {
    "priority": -1,
    "viteConfig": {
      "imports": [
        {
          "namedImports": [
            "partytownVite"
          ],
          "importPath": "@builder.io/partytown/utils"
        },
        {
          "namedImports": [
            "join"
          ],
          "importPath": "path"
        }
      ],
      "vitePlugins": [
        "partytownVite({dest: join(__dirname, 'public', '~partytown')})"
      ]
    },
    "nextSteps": [
      "------------------------------------------------",
      "You must add the next component <QwikPartytown/>;",
      "If you check more arguments ",
      "https://partytown.builder.io/configuration",
      "root.tsx -------------------------------------",
      ""
    ]
  }
}
